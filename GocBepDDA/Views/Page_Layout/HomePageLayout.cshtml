<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!-- Mobile Specific Meta
    ================================================== -->
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/Source/img/favicon.png" />

    <!-- CSS
    ================================================== -->
    <!-- Fontawesome Icon font -->
    <link rel="stylesheet" href="~/Content/Source/css/font-awesome.min.css">
    <!-- bootstrap.min css -->
    <link rel="stylesheet" href="~/Content/Source/css/bootstrap.min.css">
    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/Source/css/animate.css">
    <!-- Owl Carousel -->
    <link rel="stylesheet" href="~/Content/Source/css/owl.carousel.css">
    <!-- Grid Component css -->
    <link rel="stylesheet" href="~/Content/Source/css/component.css">
    <!-- Slit Slider css -->
    <link rel="stylesheet" href="~/Content/Source/css/slit-slider.css">
    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="~/Content/Source/css/main.css">
    <!-- Media Queries -->
    <link rel="stylesheet" href="~/Content/Source/css/media-queries.css">
    <style>
        input[type=text] {
            background-color: #2f383d;
            border-radius: 0;
            padding: 5px 10px;
            border: 0 none;
            color: #ddd;
        }
    </style>
    <!--
    Google Font
    =========================== -->
    <!-- Modernizer Script for old Browsers -->
    <script src="~/Content/Source/js/modernizr-2.6.2.min.js"></script>

</head>
<body id="body">

    <!--
    Start Preloader
    ==================================== -->
    <div id="loading-mask">
        <div class="loading-img">
            <img alt="DDA Preloader" src="~/Content/Source/img/preloader.gif" />
        </div>
    </div>
    <!--
    End Preloader
    ==================================== -->
    <!--
    Welcome Slider
    ==================================== -->
    <section id="home">

        <div id="slitSlider" class="sl-slider-wrapper">
            <div class="sl-slider">

                <!-- single slide item -->
                <div class="sl-slide" data-orientation="horizontal" data-slice1-rotation="-25" data-slice2-rotation="-25" data-slice1-scale="2" data-slice2-scale="2">
                    <div class="sl-slide-inner">
                        <div class="bg-img bg-img-1"></div>
                        <div class="carousel-caption">
                            <div>
                                <img class="wow fadeInUp" src="~/Content/Source/img/dda.png" alt="DDA">
                                <h2 data-wow-duration="500ms" data-wow-delay="500ms" class="heading animated fadeInRight">Góc bếp của Dung Dương An</h2>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /single slide item -->
                <!-- single slide item -->
                <div class="sl-slide" data-orientation="vertical" data-slice1-rotation="10" data-slice2-rotation="-15" data-slice1-scale="1.5" data-slice2-scale="1.5">
                    <div class="sl-slide-inner">
                        <div class="bg-img bg-img-2"></div>
                        <div class="carousel-caption">
                            <div>
                                <h2 class="heading animated fadeInDown">Các món ăn</h2>
                                <h3 class="animated fadeInUp">Được sử dụng những nguyên liệu tươi ngon nhất!</h3>
                                <h3 class="animated fadeInUp">Đảm bảo an toàn vệ sinh thực phẩm</h3>
                                <a class="btn btn-green animated fadeInUp" href="#">Đặt hàng ngay</a>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- /single slide item -->

            </div><!-- /sl-slider -->

            <nav id="nav-arrows" class="nav-arrows">
                <span class="nav-arrow-prev">Previous</span>
                <span class="nav-arrow-next">Next</span>
            </nav>

            <nav id="nav-dots" class="nav-dots">
                <span class="nav-dot-current"></span>
                <span></span>
                <span></span>
            </nav>

        </div><!-- /slider-wrapper -->
    </section>
    <!--/#home section-->
    <!--
    Fixed Navigation
    ==================================== -->
    <header id="navigation" class="navbar navbar-inverse">
        <div class="container">
            <div class="navbar-header">
                <!-- responsive nav button -->
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!-- /responsive nav button -->
                <!-- logo -->
                <a class="navbar-brand" href="#body">
                    <h1 id="logo">
                        <img src="~/Content/Source/img/logo-dda.png" alt="DDA" />
                    </h1>
                </a>
                <!-- /logo -->
            </div>
            <!-- main nav -->
            <nav class="collapse navbar-collapse navbar-right" role="Navigation">
                <ul id="nav" class="nav navbar-nav">
                    <li class="current"><a href="~/Home/Index">Trang chủ</a></li>
                    <li><a href="~/Cart/Cart/#cart">@Html.Action("CartPartial", "Cart")</a></li>
                    <li><a href="#menu">Thực đơn</a></li>
                    <li><a href="#contact-us">Liên hệ</a></li>
                    @Html.Action("UserPartial", "Home")
                </ul>
            </nav>
            <!-- /main nav -->

        </div>
    </header>

    <!--
    End Fixed Navigation
    ==================================== -->
    <!--
    Begin Render Body
    ==================================== -->
    <div>
        @RenderBody()
    </div>

    <!--
    End Render Body
    ==================================== -->
    <!--
    Render Partial Food Menu
    ==================================== -->

    @Html.Action("FoodMenuPartial", "Home")
    <!--
    Start Counter Section
    ==================================== -->

    <section id="counter" class="parallax-section">
        <div class="container">
            <div class="row">
                <!-- first count item -->
                <div class="col-md-3 col-sm-6 col-xs-12 text-center wow fadeInDown" data-wow-duration="500ms">
                    <div class="counters-item">
                        <div>
                            <span data-speed="3000" data-to="1929732">1.929.    732</span>
                        </div>
                        <i class="fa fa-users fa-3x"></i>
                        <h3>Khách hàng thân thiết</h3>
                    </div>
                </div>
                <!-- end first count item -->
                <!-- second count item -->
                <div class="col-md-3 col-sm-6 col-xs-12 text-center wow fadeInDown" data-wow-duration="500ms" data-wow-delay="200ms">
                    <div class="counters-item">
                        <div>
                            <span data-speed="3000" data-to="565">565</span>
                        </div>
                        <i class="fa fa-check-square fa-3x"></i>
                        <h3>Nhận xét tốt từ các chuyên gia ẩm thực</h3>
                    </div>
                </div>
                <!-- end second count item -->
                <!-- third count item -->
                <div class="col-md-3 col-sm-6 col-xs-12 text-center wow fadeInDown" data-wow-duration="500ms" data-wow-delay="400ms">
                    <div class="counters-item">
                        <div>
                            <span data-speed="3000" data-to="95">95</span>
                            <span>%</span>
                        </div>
                        <i class="fa fa-thumbs-up fa-3x"></i>
                        <h3>Phản hồi tích cực ở trong nước</h3>

                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12 text-center wow fadeInDown" data-wow-duration="500ms" data-wow-delay="400ms">
                    <div class="counters-item">
                        <div>
                            <span data-speed="3000" data-to="75">75</span>
                            <span>%</span>
                        </div>
                        <i class="fa fa-thumbs-up fa-3x"></i>
                        <h3>Phản hồi tích cực ở ngoài nước</h3>

                    </div>
                </div>

                <!-- end forth count item -->
            </div> 		<!-- end row -->
        </div>   	<!-- end container -->
    </section>   <!-- end section -->
    <!--
    Start Main Features
    ==================================== -->
    <section id="main-features">
        <div class="container">
            <div class="row">

                <!-- features item -->
                <div id="features">
                    <div class="item">

                        <div class="features-item">

                            <!-- features media -->
                            <div class="col-md-6 feature-media media-wrapper wow fadeInUp" data-wow-duration="500ms">
                                <iframe width="560" height="315" src="https://www.youtube.com/embed/UJS81tX1Thk"> </iframe>
                            </div>
                            <!-- /features media -->
                            <!-- features content -->
                            <div class="col-md-6 feature-desc wow fadeInUp" data-wow-duration="500ms" data-wow-delay="300ms">
                                <h3>Easy Poke Cake 4 ways</h3>
                                <p>DDA xin hướng dẫn bạn 4 công thức poke cake đơn giản ngon miệng!</p>
                                <a href="#" class="btn btn-transparent">Xem Thêm</a>
                                <a href="#" class="btn btn-transparent">Đặt hàng ngay</a>
                            </div>
                            <!-- /features content -->

                        </div>
                    </div>

                </div>
                <!-- /features item -->

            </div> 		<!-- End row -->
        </div>   	<!-- End container -->
    </section>   <!-- End section -->
    <!-- Start Testimonial
    =========================================== -->

    <section id="testimonial" class="parallax-section">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">

                    <!-- section title -->
                    <div class="sub-title text-center wow fadeInDown" data-wow-duration="500ms">
                        <h3>Mọi người nhận xét gì về thức ăn của DDA</h3>
                    </div>
                    <!-- /section title -->
                    <!-- testimonial wrapper -->
                    <div id="testimonials" class="wow fadeInUp" data-wow-duration="500ms" data-wow-delay="100ms">

                        <!-- testimonial single -->
                        <div class="item text-center">

                            <!-- client photo -->
                            <div class="client-thumb">
                                <img src="~/Content/Source/img/team/gordon.jpg" class="img-responsive" alt="Meghna">
                            </div>
                            <!-- /client photo -->
                            <!-- client info -->
                            <div class="client-info">
                                <div class="client-meta">
